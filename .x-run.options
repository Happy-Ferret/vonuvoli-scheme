<< execute / rust-fmt
		( rust-fmt "${@}" 2> /dev/null || true ) \
		| sed -r -n \
				-e ': b' \
				-e '/^$/ d' \
				-e '/^\#\[macro_use\]$/ d' \
				-e '/^\#\[macro_export\]$/ d' \
				-e '/^\#\[allow\(unused_macros\)\]$/ d' \
				-e '/^macro_rules!/ b d' \
				-e 'p' \
				-e 'b' \
				-e ': d' -e 'n' -e '/^ +/! b b' -e 'b d' \
		#
