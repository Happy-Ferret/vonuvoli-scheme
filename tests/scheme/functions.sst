

(apply +) => 0;

(apply + '()) => 0;
(apply + '(1) '()) => 1;
(apply + '(1) '() '()) => 1;
(apply + '(1) '() '()) => 1;

(apply + '(1 2) '()) => 3;
(apply + '(1 2) '() '()) => 3;
(apply + '(1 2) '() '()) => 3;

(apply + '(1) '(2)) => 3;
(apply + '(1) '(2) '()) => 3;
(apply + '(1) '(2) '()) => 3;

(apply + '(1 2) '(3)) => 6;
(apply + '(1 2) '() '(3)) => 6;
(apply + '(1 2) '() '(3)) => 6;


